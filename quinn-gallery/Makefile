all:
	(cd picture-c && make upload)
	(cd picture-f && make upload)
	(cd picture-i && make upload)
	(cd picture-n && make upload)
	(cd picture-s && make upload)
	(cd picture-u && make upload)

clean:
	rm -f *~
	(cd picture-c && make clean)
	(cd picture-f && make clean)
	(cd picture-i && make clean)
	(cd picture-n && make clean)
	(cd picture-s && make clean)
	(cd picture-u && make clean)
